.font_size {
    font-size: 15px;
}

.submit {
    background-color: #f60;
    border-radius: 5px;
    width: 80%;
    height: 50px;
    color: #f9f9f9;
    text-align: center;
    margin: 0 auto;
}

.feedbackText {
    width: 90%;
    min-height: 200px;
    max-height: 500px;
    height: 200px;
    margin: 5px auto 10px auto;
    padding: 5px;
    border: 1px solid #e0e0e0;
    background-color: #fff;
    text-align: center;
    display: flex;
    align-items: center;
}
